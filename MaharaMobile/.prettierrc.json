{
    "printWidth": 100,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": [
        "^react$",
        "<THIRD_PARTY_MODULES>",
        "^assets/",
        "^components/",
        "^hooks/",
        "^i18n",
        "^models/",
        "^navigation/",
        "^screens/",
        "^store/",
        "^utils/",
        "^[./].*(?<!\\.(c|le|sc)ss)$",
        "\\.(c|le|sc)ss$"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": true
}
